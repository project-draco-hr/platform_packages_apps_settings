{
switch (msg.what) {
case MSG_ENTERED_TARGET:
    if (mInsideTarget) {
      addInstruction(R.string.accessibility_tutorial_lesson_1_text_4,mTargetName);
    }
 else {
      addInstruction(R.string.accessibility_tutorial_lesson_1_text_4_exited,mTargetName);
      setFlag(FLAG_TOUCHED_TARGET,false);
    }
  break;
}
}
