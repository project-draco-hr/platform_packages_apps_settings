{
  super.onCreateOptionsMenu(menu);
  if (!mNewApn) {
    menu.add(0,MENU_DELETE,0,R.string.menu_delete).setIcon(android.R.drawable.ic_menu_delete);
  }
  menu.add(0,MENU_SAVE,0,R.string.menu_save).setIcon(android.R.drawable.ic_menu_save);
  menu.add(0,MENU_CANCEL,0,R.string.menu_cancel).setIcon(android.R.drawable.ic_menu_close_clear_cancel);
  return true;
}
