{
switch (msg.what) {
case EVENT_RESTORE_DEFAULTAPN_COMPLETE:
    fillList();
  getPreferenceScreen().setEnabled(true);
mRestoreDefaultApnMode=false;
dismissDialog(DIALOG_RESTORE_DEFAULTAPN);
Toast.makeText(ApnSettings.this,getResources().getString(R.string.restore_default_apn_completed),Toast.LENGTH_LONG).show();
break;
}
}
