{
  try {
    ByteArrayOutputStream out=new ByteArrayOutputStream();
    PrintWriter pw=new PrintWriter(out,true);
    stats.dumpLocked(null,pw,null);
    pw.flush();
    pw.close();
    out.close();
    return new String(out.toByteArray(),0);
  }
 catch (  IOException e) {
    return "IOException";
  }
}
