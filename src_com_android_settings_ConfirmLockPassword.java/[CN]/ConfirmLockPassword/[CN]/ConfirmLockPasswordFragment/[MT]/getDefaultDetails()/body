{
  boolean isProfile=Utils.isManagedProfile(UserManager.get(getActivity()),mEffectiveUserId);
  if (mIsAlpha) {
    return isProfile ? R.string.lockpassword_confirm_your_password_generic_profile : R.string.lockpassword_confirm_your_password_generic;
  }
 else {
    return isProfile ? R.string.lockpassword_confirm_your_pin_generic_profile : R.string.lockpassword_confirm_your_pin_generic;
  }
}
