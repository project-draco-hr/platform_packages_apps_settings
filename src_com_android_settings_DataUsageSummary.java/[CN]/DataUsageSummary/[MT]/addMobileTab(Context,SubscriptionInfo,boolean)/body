{
  if (subInfo != null && mMobileTagMap != null) {
    if (hasReadyMobileRadio(context,subInfo.getSubscriptionId())) {
      if (isMultiSim) {
        mTabHost.addTab(buildTabSpec(mMobileTagMap.get(subInfo.getSubscriptionId()),subInfo.getDisplayName()));
      }
 else {
        mTabHost.addTab(buildTabSpec(mMobileTagMap.get(subInfo.getSubscriptionId()),R.string.data_usage_tab_mobile));
      }
    }
  }
 else {
    if (LOGD)     Log.d(TAG,"addMobileTab: subInfoList is null");
  }
}
