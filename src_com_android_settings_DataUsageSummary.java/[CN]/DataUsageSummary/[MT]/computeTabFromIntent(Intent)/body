{
  final int networkTemplate=intent.getIntExtra(EXTRA_NETWORK_TEMPLATE,TEMPLATE_INVALID);
switch (networkTemplate) {
case TEMPLATE_MOBILE_3G_LOWER:
    return TAB_3G;
case TEMPLATE_MOBILE_4G:
  return TAB_4G;
case TEMPLATE_MOBILE_ALL:
return TAB_MOBILE;
case TEMPLATE_WIFI:
return TAB_WIFI;
default :
return null;
}
}
