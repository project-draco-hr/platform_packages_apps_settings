{
  final int networkTemplate=intent.getIntExtra(EXTRA_NETWORK_TEMPLATE,MATCH_MOBILE_ALL);
switch (networkTemplate) {
case MATCH_MOBILE_3G_LOWER:
    return TAB_3G;
case MATCH_MOBILE_4G:
  return TAB_4G;
case MATCH_MOBILE_ALL:
return TAB_MOBILE;
case MATCH_WIFI:
return TAB_WIFI;
default :
return null;
}
}
