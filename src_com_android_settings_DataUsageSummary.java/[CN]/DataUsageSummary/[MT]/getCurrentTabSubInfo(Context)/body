{
  if (mSubInfoList != null && mTabHost != null) {
    final int currentTagIndex=mTabHost.getCurrentTab();
    int i=0;
    for (    SubInfoRecord subInfo : mSubInfoList) {
      if (hasReadyMobileRadio(context,subInfo.getSubscriptionId())) {
        if (i++ == currentTagIndex) {
          return subInfo;
        }
      }
    }
  }
  return null;
}
