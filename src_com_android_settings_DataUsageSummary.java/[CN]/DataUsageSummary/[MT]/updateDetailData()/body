{
  if (LOGD)   Log.d(TAG,"updateDetailData()");
  new AsyncTask<Void,Void,NetworkStats>(){
    @Override protected NetworkStats doInBackground(    Void... params){
      try {
        final long[] range=mChart.getInspectRange();
        return mStatsService.getSummaryForAllUid(mTemplate,range[0],range[1]);
      }
 catch (      RemoteException e) {
        Log.w(TAG,"problem reading stats");
      }
      return null;
    }
    @Override protected void onPostExecute(    NetworkStats stats){
      if (stats != null) {
        mAdapter.bindStats(stats);
      }
    }
  }
.execute();
}
