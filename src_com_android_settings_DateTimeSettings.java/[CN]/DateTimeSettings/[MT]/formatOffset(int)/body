{
  off=off / 1000 / 60;
  char[] buf=new char[9];
  buf[0]='G';
  buf[1]='M';
  buf[2]='T';
  if (off < 0) {
    buf[3]='-';
    off=-off;
  }
 else {
    buf[3]='+';
  }
  int hours=off / 60;
  int minutes=off % 60;
  buf[4]=(char)('0' + hours / 10);
  buf[5]=(char)('0' + hours % 10);
  buf[6]=':';
  buf[7]=(char)('0' + minutes / 10);
  buf[8]=(char)('0' + minutes % 10);
  return buf;
}
