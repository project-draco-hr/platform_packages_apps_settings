{
  if (mAdbDialog != null) {
    mAdbDialog.dismiss();
    mAdbDialog=null;
  }
  if (mAdbKeysDialog != null) {
    mAdbKeysDialog.dismiss();
    mAdbKeysDialog=null;
  }
  if (mEnableDialog != null) {
    mEnableDialog.dismiss();
    mEnableDialog=null;
  }
  if (mLogpersistClearDialog != null) {
    mLogpersistClearDialog.dismiss();
    mLogpersistClearDialog=null;
  }
}
