{
  String value=null;
  Cursor c=null;
  try {
    ContentResolver resolver=getActivity().getContentResolver();
    c=resolver.query(GOOGLE_SETTINGS_CONTENT_URI,new String[]{"value"},"name=?",new String[]{name},null);
    if (c != null && c.moveToNext())     value=c.getString(0);
  }
  finally {
    if (c != null)     c.close();
  }
  return Integer.parseInt(value);
}
