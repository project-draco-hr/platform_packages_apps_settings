{
  if (dialog == mAdbDialog) {
    if (!mDialogClicked) {
      mEnableAdb.setChecked(false);
    }
    mAdbDialog=null;
  }
 else   if (dialog == mEnableDialog) {
    if (!mDialogClicked) {
      mSwitchBar.setChecked(false);
    }
    mEnableDialog=null;
  }
 else   if (dialog == mLogpersistClearDialog) {
    mLogpersistClearDialog=null;
  }
}
