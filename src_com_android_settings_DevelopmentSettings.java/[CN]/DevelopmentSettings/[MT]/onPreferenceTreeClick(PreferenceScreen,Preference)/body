{
  if (!TextUtils.isEmpty(SystemProperties.get("ro.monkey"))) {
    return false;
  }
  if (preference == mEnableAdb) {
    Settings.System.putInt(getContentResolver(),Settings.System.ADB_ENABLED,mEnableAdb.isChecked() ? 1 : 0);
  }
 else   if (preference == mKeepScreenOn) {
    Settings.System.putInt(getContentResolver(),Settings.System.STAY_ON_WHILE_PLUGGED_IN,mKeepScreenOn.isChecked() ? 1 : 0);
  }
  return false;
}
