{
  int val=-1;
  try {
    val=getGoogleSettingValue(WEBVIEW_DATA_REDUCTION_PROXY);
  }
 catch (  Exception e) {
  }
  if (val == -1) {
    val=Settings.Secure.getInt(getActivity().getContentResolver(),Settings.Secure.WEBVIEW_DATA_REDUCTION_PROXY,0);
  }
  updateSwitchPreference(mWebViewDataReductionProxy,val != 0);
}
