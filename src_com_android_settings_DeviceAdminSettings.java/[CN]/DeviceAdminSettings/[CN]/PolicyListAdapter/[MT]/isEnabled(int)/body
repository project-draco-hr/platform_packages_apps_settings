{
  DeviceAdminInfo info=mAvailableAdmins.get(position);
  String packageName=info.getPackageName();
  ComponentName component=info.getComponent();
  if (mActiveAdmins.contains(component) && (packageName.equals(mDeviceOwnerPkg) || component.equals(mProfileOwnerComponent))) {
    return false;
  }
 else {
    return true;
  }
}
