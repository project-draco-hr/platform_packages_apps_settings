{
  String component=Settings.Secure.getString(resolver,SCREENSAVER_COMPONENT);
  Log.v(TAG,"component=" + component);
  if (component != null) {
    ComponentName cn=ComponentName.unflattenFromString(component);
    Log.v(TAG,"cn=" + cn);
    Intent intent=new Intent(Intent.ACTION_MAIN).setComponent(cn).addFlags(Intent.FLAG_ACTIVITY_EXCLUDE_FROM_RECENTS).putExtra("android.dreams.TEST",true);
    getContext().startActivity(intent);
  }
}
