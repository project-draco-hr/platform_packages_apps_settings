{
  return new Intent(ctx,EncryptionInterstitial.class).putExtra(EXTRA_PREFS_SHOW_BUTTON_BAR,true).putExtra(EXTRA_PREFS_SET_BACK_TEXT,(String)null).putExtra(EXTRA_PREFS_SET_NEXT_TEXT,ctx.getString(R.string.encryption_continue_button)).putExtra(EXTRA_PASSWORD_QUALITY,quality).putExtra(EXTRA_SHOW_FRAGMENT_TITLE_RESID,R.string.encryption_interstitial_header).putExtra(EXTRA_REQUIRE_PASSWORD,requirePasswordDefault).putExtra(EXTRA_UNLOCK_METHOD_INTENT,unlockMethodIntent);
}
