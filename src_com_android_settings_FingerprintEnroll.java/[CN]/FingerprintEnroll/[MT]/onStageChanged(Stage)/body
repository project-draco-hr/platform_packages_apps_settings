{
switch (stage) {
case EnrollingOnboard:
case EnrollingFindSensor:
    mEnrollmentSteps=-1;
  mEnrolling=false;
break;
case EnrollingStart:
mEnrollmentSteps=-1;
long challenge=0x12345;
mFingerprintManager.enroll(challenge,mEnrollmentCallback,mEnrollmentCancel,0);
mProgressBar.setProgress(0);
mEnrolling=true;
startFingerprintAnimator();
break;
case EnrollingRepeat:
break;
case EnrollingFinish:
stopFingerprintAnimator();
mEnrolling=false;
break;
default :
break;
}
}
