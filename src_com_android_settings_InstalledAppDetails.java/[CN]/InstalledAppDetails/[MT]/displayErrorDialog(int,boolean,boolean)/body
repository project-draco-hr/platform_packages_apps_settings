{
  new AlertDialog.Builder(this).setTitle(getString(R.string.app_not_found_dlg_title)).setIcon(android.R.drawable.ic_dialog_alert).setMessage(getString(msgId)).setNeutralButton(getString(R.string.dlg_ok),new DialogInterface.OnClickListener(){
    public void onClick(    DialogInterface dialog,    int which){
      setIntentAndFinish(finish,changed);
    }
  }
).show();
}
