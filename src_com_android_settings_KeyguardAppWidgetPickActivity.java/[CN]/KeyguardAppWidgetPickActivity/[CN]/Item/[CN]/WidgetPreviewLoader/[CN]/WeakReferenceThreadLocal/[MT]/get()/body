{
  WeakReference<T> reference=mThreadLocal.get();
  T obj;
  if (reference == null) {
    obj=initialValue();
    mThreadLocal.set(new WeakReference<T>(obj));
    return obj;
  }
 else {
    obj=reference.get();
    if (obj == null) {
      obj=initialValue();
      mThreadLocal.set(new WeakReference<T>(obj));
    }
    return obj;
  }
}
