{
  String pkgName=info.packageName;
  if (shouldBeInList(mFilterApps,info)) {
    int newIdx=Collections.binarySearch(mAppLocalList,info,getAppComparator(mSortOrder));
    if (newIdx >= 0) {
      if (localLOGV)       Log.i(TAG,"Strange. Package:" + pkgName + " is not new");
      return false;
    }
    newIdx=-newIdx - 1;
    addFilterListLocked(newIdx,info,label);
    adjustIndex();
    return true;
  }
  return false;
}
