{
  if (pkgName == null) {
    Log.w(TAG,"Adding null pkg to List Adapter");
    return;
  }
  ApplicationInfo info;
  try {
    info=mPm.getApplicationInfo(pkgName,0);
  }
 catch (  NameNotFoundException e) {
    Log.w(TAG,"Ignoring non-existent package:" + pkgName);
    return;
  }
  if (info == null) {
    Log.i(TAG,"Null ApplicationInfo for package:" + pkgName);
    return;
  }
  int newIdx=Collections.binarySearch(mAppLocalList,info,getAppComparator(mSortOrder));
  if (newIdx >= 0) {
    Log.i(TAG,"Strange. Package:" + pkgName + " is not new");
    return;
  }
  newIdx=-newIdx - 1;
  mAppLocalList.add(newIdx,info);
  mAppPropMap.put(info.packageName,new AppInfo(pkgName,newIdx,info.loadLabel(mPm),info.loadIcon(mPm),ps));
  adjustIndex();
  notifyDataSetChanged();
}
