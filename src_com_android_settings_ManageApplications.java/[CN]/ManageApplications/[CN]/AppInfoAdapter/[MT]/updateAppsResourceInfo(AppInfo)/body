{
  if ((pInfo == null) || (pInfo.pkgName == null)) {
    Log.w(TAG,"Null info when refreshing icon in List Adapter");
    return false;
  }
  AppInfo aInfo=mAppPropMap.get(pInfo.pkgName);
  if (aInfo != null) {
    aInfo.refreshIcon(pInfo);
    notifyDataSetChanged();
    return true;
  }
  return false;
}
