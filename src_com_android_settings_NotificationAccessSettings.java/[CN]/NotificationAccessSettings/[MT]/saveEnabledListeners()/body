{
  StringBuilder sb=null;
  for (  ComponentName cn : mEnabledListeners) {
    if (sb == null) {
      sb=new StringBuilder();
    }
 else {
      sb.append(':');
    }
    sb.append(cn.flattenToString());
  }
  Settings.Secure.putString(mCR,Settings.Secure.ENABLED_NOTIFICATION_LISTENERS,sb != null ? sb.toString() : "");
}
