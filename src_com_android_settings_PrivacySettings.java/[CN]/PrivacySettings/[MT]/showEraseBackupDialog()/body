{
  mBackup.setChecked(true);
  mDialogType=DIALOG_ERASE_BACKUP;
  CharSequence msg=getResources().getText(R.string.backup_erase_dialog_message);
  mConfirmDialog=new AlertDialog.Builder(getActivity()).setMessage(msg).setTitle(R.string.backup_erase_dialog_title).setIconAttribute(android.R.attr.alertDialogIcon).setPositiveButton(android.R.string.ok,this).setNegativeButton(android.R.string.cancel,this).show();
}
