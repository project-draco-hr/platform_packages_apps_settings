{
  final String key=preference.getKey();
  if (KEY_UNLOCK_SET_OR_CHANGE.equals(key)) {
    Bundle extras=new Bundle();
    extras.putInt(Intent.EXTRA_USER_ID,mProfileUserId);
    startFragment(this,"com.android.settings.ChooseLockGeneric$ChooseLockGenericFragment",R.string.lock_settings_picker_title,SET_OR_CHANGE_LOCK_METHOD_REQUEST,extras);
    return true;
  }
 else   if (KEY_UNIFICATION.equals(key)) {
    UnificationConfirmationDialog dialog=UnificationConfirmationDialog.newIntance(mProfileUserId);
    dialog.show(getChildFragmentManager(),TAG_UNIFICATION_DIALOG);
    return true;
  }
  return super.onPreferenceTreeClick(preference);
}
