{
  int rate=mCellInfoListRateHandler.getRate();
  String rateStr;
switch (rate) {
case Integer.MAX_VALUE:
    rateStr="Off";
  break;
case 0:
rateStr="Immediate";
break;
default :
rateStr=(new Integer(rate)).toString() + "ms";
break;
}
cellInfoListRateButton.setText("CellInfoListRate " + rateStr);
}
