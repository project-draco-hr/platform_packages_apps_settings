{
  String neighborings="";
  if (cids != null) {
    if (cids.isEmpty()) {
      neighborings="no neighboring cells";
    }
 else {
      for (      NeighboringCellInfo cell : cids) {
        neighborings+="{" + Integer.toHexString(cell.getCid()) + "@"+ cell.getRssi()+ "} ";
      }
    }
  }
 else {
    neighborings="unknown";
  }
  mNeighboringCids.setText(neighborings);
}
