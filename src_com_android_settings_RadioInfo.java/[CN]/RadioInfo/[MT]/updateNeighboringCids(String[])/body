{
  if (cids != null && cids.length > 0 && cids[0] != null) {
    int size=Integer.parseInt(cids[0]);
    String neiborings;
    if (size > 0) {
      neiborings="{";
      for (int i=1; i <= size; i++) {
        neiborings+=cids[i] + ", ";
      }
      neiborings+="}";
    }
 else {
      neiborings="none";
    }
    mNeighboringCids.setText(neiborings);
  }
 else {
    mNeighboringCids.setText("unknown");
  }
}
