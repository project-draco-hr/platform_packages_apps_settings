{
  String s;
  Resources r=getResources();
  s=phone.getDeviceId();
  if (s == null)   s=r.getString(R.string.radioInfo_unknown);
  mImei.setText(s);
  s=phone.getSubscriberId();
  if (s == null)   s=r.getString(R.string.radioInfo_unknown);
  mImsi.setText(s);
  s=phone.getLine1Number();
  if (s == null)   s=r.getString(R.string.radioInfo_unknown);
  number.setText(s);
}
