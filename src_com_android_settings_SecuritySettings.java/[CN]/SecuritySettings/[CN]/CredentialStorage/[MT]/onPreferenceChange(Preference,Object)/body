{
  if (preference == mAccessCheckBox) {
    if ((Boolean)value) {
      showUnlockDialog();
    }
 else {
      lock();
    }
    return true;
  }
 else   if (preference == mEncryptedFSEnabled) {
    Boolean bval=(Boolean)value;
    mWillEnableEncryptedFS=bval.booleanValue();
    showSwitchEncryptedFSDialog();
  }
  return true;
}
