{
  if (preference == mInstallButton) {
    Credentials.getInstance().installFromSdCard(SecuritySettings.this.getActivity());
  }
 else   if (preference == mPasswordButton) {
    showPasswordDialog();
  }
 else   if (preference == mResetButton) {
    showResetDialog();
  }
 else {
    return false;
  }
  return true;
}
