{
  if (isPkcs12Keystore()) {
    return CertTool.getInstance().addPkcs12Keystore(mItemList.get(0),mPassword,mName);
  }
 else {
    Keystore ks=Keystore.getInstance();
    for (int i=0, count=mItemList.size(); i < count; i++) {
      byte[] blob=mItemList.get(i);
      int ret=ks.put(mNamespaceList.get(i),mName,new String(blob));
      if (ret < 0)       return ret;
    }
  }
  return 0;
}
