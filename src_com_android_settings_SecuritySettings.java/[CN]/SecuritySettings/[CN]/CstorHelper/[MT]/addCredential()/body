{
  if (mCstorAddCredentialHelper.saveToStorage() != 0) {
    mView=mCstorAddCredentialHelper.mView;
    if (mCstorAddCredentialHelper.isPkcs12Keystore()) {
      showError(R.string.cstor_password_error);
    }
 else {
      showError(R.string.cstor_storage_error);
    }
    Log.d("CSTOR","failed to add credential");
    return false;
  }
  Log.d("CSTOR","credential is added: " + mCstorAddCredentialHelper.getName());
  String formatString=getString(R.string.cstor_is_added);
  String message=String.format(formatString,mCstorAddCredentialHelper.getName());
  Toast.makeText(SecuritySettings.this,message,Toast.LENGTH_LONG).show();
  return true;
}
