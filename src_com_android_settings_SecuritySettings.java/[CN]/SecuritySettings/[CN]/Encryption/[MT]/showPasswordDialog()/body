{
  View view=View.inflate(SecuritySettings.this.getActivity(),R.layout.credentials_dialog,null);
  view.findViewById(R.id.new_passwords).setVisibility(View.VISIBLE);
  Dialog dialog=new AlertDialog.Builder(SecuritySettings.this.getActivity()).setView(view).setTitle(R.string.credentials_set_password).setPositiveButton(android.R.string.ok,this).setNegativeButton(android.R.string.cancel,this).create();
  dialog.setOnDismissListener(this);
  dialog.show();
}
