{
  super.onResume();
  createPreferenceHierarchy();
  final LockPatternUtils lockPatternUtils=mLockPatternUtils;
  if (mVisiblePattern != null) {
    mVisiblePattern.setChecked(lockPatternUtils.isVisiblePatternEnabled(MY_USER_ID));
  }
  if (mPowerButtonInstantlyLocks != null) {
    mPowerButtonInstantlyLocks.setChecked(lockPatternUtils.getPowerButtonInstantlyLocks(MY_USER_ID));
  }
  updateOwnerInfo();
}
