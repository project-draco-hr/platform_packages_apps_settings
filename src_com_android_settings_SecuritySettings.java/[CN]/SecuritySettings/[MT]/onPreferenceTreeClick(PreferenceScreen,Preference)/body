{
  final String key=preference.getKey();
  if (KEY_UNLOCK_SET_OR_CHANGE.equals(key)) {
    startFragment(this,"com.android.settings.ChooseLockGeneric$ChooseLockGenericFragment",R.string.lock_settings_picker_title,SET_OR_CHANGE_LOCK_METHOD_REQUEST,null);
  }
 else   if (KEY_BIOMETRIC_WEAK_IMPROVE_MATCHING.equals(key)) {
    ChooseLockSettingsHelper helper=new ChooseLockSettingsHelper(this.getActivity(),this);
    if (!helper.launchConfirmationActivity(CONFIRM_EXISTING_FOR_BIOMETRIC_WEAK_IMPROVE_REQUEST,null,null)) {
      startBiometricWeakImprove();
    }
  }
 else   if (KEY_TRUST_AGENT.equals(key)) {
    ChooseLockSettingsHelper helper=new ChooseLockSettingsHelper(this.getActivity(),this);
    mClickedTrustAgentPreference=preference;
    if (!helper.launchConfirmationActivity(CHANGE_TRUST_AGENT_SETTINGS,null,null) && preference.getIntent() != null) {
      startActivity(preference.getIntent());
      mClickedTrustAgentPreference=null;
    }
  }
 else {
    return super.onPreferenceTreeClick(preferenceScreen,preference);
  }
  return true;
}
