{
  Intent intent=super.onBuildStartFragmentIntent(fragmentName,args,titleRes,shortTitleRes);
  if (DataUsageSummary.class.getName().equals(fragmentName) || PowerUsageSummary.class.getName().equals(fragmentName) || AccountSyncSettings.class.getName().equals(fragmentName)|| UserDictionarySettings.class.getName().equals(fragmentName)) {
    intent.putExtra(EXTRA_THEME,android.R.style.Theme_Holo_SolidActionBar);
  }
  intent.setClass(this,SubSettings.class);
  return intent;
}
