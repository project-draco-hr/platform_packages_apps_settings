{
  mNeedToRevertToInitialFragment=false;
  mSearchResultsFragment=null;
  mSearchMenuItemExpanded=false;
  getFragmentManager().popBackStackImmediate(SettingsActivity.BACK_STACK_PREFS,FragmentManager.POP_BACK_STACK_INCLUSIVE);
  if (mSearchMenuItem != null) {
    mSearchMenuItem.collapseActionView();
  }
}
