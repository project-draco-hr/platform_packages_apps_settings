{
  final String themeName=intent.getStringExtra(EXTRA_THEME);
  if (THEME_HOLO.equalsIgnoreCase(themeName) || THEME_MATERIAL.equalsIgnoreCase(themeName) || THEME_MATERIAL_BLUE.equalsIgnoreCase(themeName)) {
    return R.style.SetupWizardTheme;
  }
 else {
    return R.style.SetupWizardTheme_Light;
  }
}
