{
  if (mUserManager.hasUserRestriction(UserManager.DISALLOW_CONFIG_CREDENTIALS)) {
    return inflater.inflate(R.layout.credentials_disallowed_preference_screen,parent,false);
  }
  mTabHost=(TabHost)inflater.inflate(R.layout.trusted_credentials,parent,false);
  mTabHost.setup();
  addTab(Tab.SYSTEM);
  addTab(Tab.USER);
  if (getActivity().getIntent() != null && USER_ACTION.equals(getActivity().getIntent().getAction())) {
    mTabHost.setCurrentTabByTag(Tab.USER.mTag);
  }
  return mTabHost;
}
