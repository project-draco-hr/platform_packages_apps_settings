{
  ArrayMap<String,PackageEntry> entriesForProfile=mPackageEntryMap.get(profileId);
  if (entriesForProfile == null) {
    Log.w(TAG,"getPackageEntry fails for package " + packageName + " of user "+ profileId+ ": user does not seem to be valid.");
    return null;
  }
  final PackageEntry entry=entriesForProfile.get(packageName);
  if (entry == null) {
    Log.w(TAG,"getPackageEntry fails for package " + packageName + " of user "+ profileId+ ": package does not exist.");
  }
  return entry;
}
