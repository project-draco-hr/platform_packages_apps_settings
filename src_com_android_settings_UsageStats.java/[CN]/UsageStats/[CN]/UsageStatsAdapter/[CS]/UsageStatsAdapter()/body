{
  Calendar cal=Calendar.getInstance();
  cal.add(Calendar.DAY_OF_YEAR,-5);
  final android.app.usage.UsageStats stats=mUsageStatsManager.getRecentStatsSince(cal.getTimeInMillis());
  if (stats == null) {
    return;
  }
  final int pkgCount=stats.getPackageCount();
  for (int i=0; i < pkgCount; i++) {
    final PackageUsageStats pkgStats=stats.getPackage(i);
    try {
      ApplicationInfo appInfo=mPm.getApplicationInfo(pkgStats.getPackageName(),0);
      String label=appInfo.loadLabel(mPm).toString();
      mAppLabelMap.put(pkgStats.getPackageName(),label);
      mPackageStats.add(pkgStats);
    }
 catch (    NameNotFoundException e) {
    }
  }
  mAppLabelComparator=new AppNameComparator(mAppLabelMap);
  sortList();
}
