{
  if (user.isManagedProfile()) {
    Bitmap b=BitmapFactory.decodeResource(context.getResources(),com.android.internal.R.drawable.ic_corp_icon);
    return CircleFramedDrawable.getInstance(context,b);
  }
  if (user.iconPath != null) {
    Bitmap icon=um.getUserIcon(user.id);
    if (icon != null) {
      return CircleFramedDrawable.getInstance(context,icon);
    }
  }
  return CircleFramedDrawable.getInstance(context,UserIcons.convertToBitmap(UserIcons.getDefaultUserIcon(user.id,false)));
}
