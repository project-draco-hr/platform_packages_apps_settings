{
  if (preference == mAutoSyncCheckbox) {
    ContentResolver.setMasterSyncAutomatically(mAutoSyncCheckbox.isChecked());
    onSyncStateUpdated();
  }
 else   if (preference instanceof AccountPreference) {
    startAccountSettings((AccountPreference)preference);
  }
 else {
    return false;
  }
  return true;
}
