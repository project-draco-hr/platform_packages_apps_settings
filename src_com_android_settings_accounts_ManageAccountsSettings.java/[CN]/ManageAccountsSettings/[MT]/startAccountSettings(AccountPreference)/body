{
  Bundle args=new Bundle();
  args.putParcelable(AccountSyncSettings.ACCOUNT_KEY,acctPref.getAccount());
  ((PreferenceActivity)getActivity()).startPreferencePanel(AccountSyncSettings.class.getCanonicalName(),args,R.string.account_sync_settings_title,acctPref.getAccount().name,this,REQUEST_SHOW_SYNC_SETTINGS);
}
