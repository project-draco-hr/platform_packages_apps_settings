{
  if (mIcon == null) {
    if (mApkFile.exists()) {
      mIcon=mInfo.loadIcon(mLoader.mPm);
      return mIcon;
    }
 else {
      mMounted=false;
    }
  }
 else   if (!mMounted) {
    if (mApkFile.exists()) {
      mMounted=true;
      mIcon=mInfo.loadIcon(mLoader.mPm);
      return mIcon;
    }
  }
 else {
    return mIcon;
  }
  return mLoader.getContext().getResources().getDrawable(android.R.drawable.sym_def_app_icon);
}
