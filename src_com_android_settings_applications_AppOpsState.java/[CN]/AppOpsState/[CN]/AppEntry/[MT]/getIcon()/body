{
  if (mIcon == null) {
    if (mApkFile.exists()) {
      mIcon=mInfo.loadIcon(mState.mPm);
      return mIcon;
    }
 else {
      mMounted=false;
    }
  }
 else   if (!mMounted) {
    if (mApkFile.exists()) {
      mMounted=true;
      mIcon=mInfo.loadIcon(mState.mPm);
      return mIcon;
    }
  }
 else {
    return mIcon;
  }
  return mState.mContext.getResources().getDrawable(android.R.drawable.sym_def_app_icon);
}
