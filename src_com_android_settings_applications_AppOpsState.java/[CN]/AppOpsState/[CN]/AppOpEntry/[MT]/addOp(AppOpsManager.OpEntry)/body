{
  mApp.addOp(op);
  for (int i=0; i < mOps.size(); i++) {
    AppOpsManager.OpEntry pos=mOps.get(i);
    if (pos.isRunning() != op.isRunning()) {
      if (op.isRunning()) {
        mOps.add(i,op);
        return;
      }
      continue;
    }
    if (pos.getTime() < op.getTime()) {
      mOps.add(i,op);
      return;
    }
  }
  mOps.add(op);
}
