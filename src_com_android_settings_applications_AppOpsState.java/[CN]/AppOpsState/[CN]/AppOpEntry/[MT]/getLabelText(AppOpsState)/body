{
  if (getNumOpEntry() == 1) {
    return state.mOpNames[getOpEntry(0).getOp()];
  }
 else {
    StringBuilder builder=new StringBuilder();
    for (int i=0; i < getNumOpEntry(); i++) {
      if (i > 0) {
        builder.append(", ");
      }
      builder.append(state.mOpNames[getOpEntry(i).getOp()]);
    }
    return builder.toString();
  }
}
