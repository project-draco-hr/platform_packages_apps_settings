{
  if (this.label == null || !this.mounted) {
    if (!this.apkFile.exists()) {
      this.mounted=false;
      this.label=info.packageName;
    }
 else {
      this.mounted=true;
      CharSequence label=info.loadLabel(context.getPackageManager());
      this.label=label != null ? label.toString() : info.packageName;
    }
  }
}
