{
  mContext=app;
  mPm=mContext.getPackageManager();
  mIpm=AppGlobals.getPackageManager();
  mUm=(UserManager)app.getSystemService(Context.USER_SERVICE);
  for (  UserHandle user : mUm.getUserProfiles()) {
    mEntriesMap.put(user.getIdentifier(),new HashMap<String,AppEntry>());
  }
  mThread=new HandlerThread("ApplicationsState.Loader",Process.THREAD_PRIORITY_BACKGROUND);
  mThread.start();
  mBackgroundHandler=new BackgroundHandler(mThread.getLooper());
  mOwnerRetrieveFlags=PackageManager.GET_UNINSTALLED_PACKAGES | PackageManager.GET_DISABLED_COMPONENTS | PackageManager.GET_DISABLED_UNTIL_USED_COMPONENTS;
  mRetrieveFlags=PackageManager.GET_DISABLED_COMPONENTS | PackageManager.GET_DISABLED_UNTIL_USED_COMPONENTS;
synchronized (mEntriesMap) {
    try {
      mEntriesMap.wait(1);
    }
 catch (    InterruptedException e) {
    }
  }
}
