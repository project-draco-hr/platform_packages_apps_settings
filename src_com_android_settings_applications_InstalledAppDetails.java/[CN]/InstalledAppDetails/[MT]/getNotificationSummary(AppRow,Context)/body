{
  if (appRow.banned) {
    return context.getString(R.string.notifications_disabled);
  }
 else   if (appRow.appImportance > NotificationListenerService.Ranking.IMPORTANCE_NONE && appRow.appImportance < NotificationListenerService.Ranking.IMPORTANCE_DEFAULT) {
    return context.getString(R.string.notifications_silenced);
  }
  return "";
}
