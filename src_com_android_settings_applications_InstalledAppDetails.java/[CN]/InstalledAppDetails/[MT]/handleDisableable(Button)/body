{
  boolean disableable=false;
  if (mHomePackages.contains(mAppEntry.info.packageName) || isThisASystemPackage()) {
    button.setText(R.string.disable_text);
  }
 else   if (mAppEntry.info.enabled) {
    button.setText(R.string.disable_text);
    disableable=true;
  }
 else {
    button.setText(R.string.enable_text);
    disableable=true;
  }
  return disableable;
}
