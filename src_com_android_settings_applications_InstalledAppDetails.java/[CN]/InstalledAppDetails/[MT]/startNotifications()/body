{
  getActivity().startActivity(new Intent(Settings.ACTION_APP_NOTIFICATION_SETTINGS).addFlags(Intent.FLAG_ACTIVITY_CLEAR_TOP).putExtra(AppInfoWithHeader.EXTRA_HIDE_INFO_BUTTON,true).putExtra(Settings.EXTRA_APP_PACKAGE,mAppEntry.info.packageName).putExtra(Settings.EXTRA_APP_UID,mAppEntry.info.uid));
}
