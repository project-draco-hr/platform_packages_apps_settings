{
  if (icons == null) {
    return;
  }
  Set<String> keys=icons.keySet();
  boolean changed=false;
  for (  String key : keys) {
    Drawable ic=icons.get(key);
    if (ic != null) {
      AppInfo aInfo=mCache.getEntry(key);
      if (aInfo != null) {
        aInfo.refreshIcon(ic);
        changed=true;
      }
    }
  }
  if (changed) {
    notifyDataSetChanged();
  }
}
