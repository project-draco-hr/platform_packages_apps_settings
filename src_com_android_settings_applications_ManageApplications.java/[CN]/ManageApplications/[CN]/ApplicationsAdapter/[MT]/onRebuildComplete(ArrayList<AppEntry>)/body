{
  if (mManageApplications.mLoadingContainer.getVisibility() == View.VISIBLE) {
    mManageApplications.mLoadingContainer.startAnimation(AnimationUtils.loadAnimation(mContext,android.R.anim.fade_out));
    mManageApplications.mListContainer.startAnimation(AnimationUtils.loadAnimation(mContext,android.R.anim.fade_in));
  }
  mManageApplications.mListContainer.setVisibility(View.VISIBLE);
  mManageApplications.mLoadingContainer.setVisibility(View.GONE);
  mBaseEntries=apps;
  mEntries=applyPrefixFilter(mCurFilterPrefix,mBaseEntries);
  notifyDataSetChanged();
}
