{
  if (mTab.mLoadingContainer.getVisibility() == View.VISIBLE) {
    mTab.mLoadingContainer.startAnimation(AnimationUtils.loadAnimation(mContext,android.R.anim.fade_out));
    mTab.mListContainer.startAnimation(AnimationUtils.loadAnimation(mContext,android.R.anim.fade_in));
  }
  mTab.mListContainer.setVisibility(View.VISIBLE);
  mTab.mLoadingContainer.setVisibility(View.GONE);
  mWaitingForData=false;
  mBaseEntries=apps;
  mEntries=applyPrefixFilter(mCurFilterPrefix,mBaseEntries);
  notifyDataSetChanged();
  mTab.updateStorageUsage();
}
