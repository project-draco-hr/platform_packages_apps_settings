{
  mLoadingContainer.startAnimation(AnimationUtils.loadAnimation(mOwner.getActivity(),android.R.anim.fade_out));
  mRunningProcessesView.startAnimation(AnimationUtils.loadAnimation(mOwner.getActivity(),android.R.anim.fade_in));
  mRunningProcessesView.setVisibility(View.VISIBLE);
  mLoadingContainer.setVisibility(View.GONE);
}
