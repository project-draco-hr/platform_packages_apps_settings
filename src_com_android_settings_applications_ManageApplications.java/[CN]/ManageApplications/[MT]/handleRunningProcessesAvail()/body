{
  if (mCurView == VIEW_RUNNING) {
    mLoadingContainer.startAnimation(AnimationUtils.loadAnimation(getActivity(),android.R.anim.fade_out));
    mRunningProcessesView.startAnimation(AnimationUtils.loadAnimation(getActivity(),android.R.anim.fade_in));
    mRunningProcessesView.setVisibility(View.VISIBLE);
    mLoadingContainer.setVisibility(View.GONE);
  }
}
