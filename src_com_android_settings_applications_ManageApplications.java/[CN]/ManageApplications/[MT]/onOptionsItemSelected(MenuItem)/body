{
  int menuId=item.getItemId();
switch (item.getItemId()) {
case SORT_ORDER_ALPHA:
case SORT_ORDER_SIZE:
    mSortOrder=menuId;
  if (mApplications != null) {
    mApplications.rebuild(mSortOrder);
  }
break;
case R.id.advanced:
((SettingsActivity)getActivity()).startPreferencePanel(AdvancedAppSettings.class.getName(),null,R.string.advanced_apps,null,this,ADVANCED_SETTINGS);
return true;
default :
return false;
}
updateOptionsMenu();
return true;
}
