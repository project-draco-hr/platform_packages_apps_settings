{
  mPackage=pkgName;
  mUid=uid;
  mName=procName;
  mBgDuration=mRunDuration=duration;
  mAvgBgMem=mMaxBgMem=mAvgRunMem=mMaxRunMem=mem;
  mBgWeight=mRunWeight=((double)memDuration) * mem;
  if (DEBUG)   Log.d(TAG,"New proc entry " + procName + ": dur="+ mBgDuration+ " avgpss="+ mAvgBgMem+ " weight="+ mBgWeight);
}
