{
  super.onCreate(icicle);
  if (icicle != null) {
    mStats=sStatsXfer;
  }
  addPreferencesFromResource(R.xml.process_stats_summary);
  mProcessStats=IProcessStats.Stub.asInterface(ServiceManager.getService(ProcessStats.SERVICE_NAME));
