{
  if (mUser == null) {
    return super.loadIcon(context,state);
  }
  if (mUser.mIcon != null) {
    ConstantState constState=mUser.mIcon.getConstantState();
    if (constState == null) {
      return mUser.mIcon;
    }
 else {
      return constState.newDrawable();
    }
  }
  return context.getResources().getDrawable(com.android.internal.R.drawable.ic_menu_cc);
}
