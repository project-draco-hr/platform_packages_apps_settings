{
  mReceivedPairingCanceled=true;
  TextView messageView=(TextView)findViewById(R.id.message);
  messageView.setText(getString(R.string.bluetooth_pairing_error_message,mLocalManager.getLocalDeviceManager().getName(mAddress)));
  mPinView.setVisibility(View.GONE);
  mPinView.clearFocus();
  mPinView.removeTextChangedListener(this);
  mOkButton.setEnabled(true);
  mAlert.getButton(DialogInterface.BUTTON_NEGATIVE).setVisibility(View.GONE);
}
