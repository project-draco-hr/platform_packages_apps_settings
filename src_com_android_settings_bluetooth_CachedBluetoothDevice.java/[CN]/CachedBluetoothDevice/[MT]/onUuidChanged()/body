{
  updateProfiles();
  if (DEBUG) {
    Log.e(TAG,"onUuidChanged: Time since last connect" + (SystemClock.elapsedRealtime() - mConnectAttempted));
  }
  if (!mProfiles.isEmpty() && (mConnectAttempted + MAX_UUID_DELAY_FOR_AUTO_CONNECT) > SystemClock.elapsedRealtime()) {
    connectWithoutResettingTimer(false);
  }
  dispatchAttributesChanged();
}
