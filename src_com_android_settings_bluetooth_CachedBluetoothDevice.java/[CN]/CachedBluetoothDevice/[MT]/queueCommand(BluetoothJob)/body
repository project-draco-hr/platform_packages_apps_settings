{
synchronized (workQueue) {
    if (D) {
      Log.d(TAG,workQueue.toString());
    }
    boolean processNow=pruneQueue(job);
    if (D) {
      Log.d(TAG,"Adding: " + job.toString());
    }
    workQueue.add(job);
    if (workQueue.size() == 1 || processNow) {
      processCommands();
    }
  }
}
