{
  SharedPreferences.Editor editor=mContext.getSharedPreferences(MESSAGE_REJECTION_COUNT_PREFS_NAME,Context.MODE_PRIVATE).edit();
  if (mMessageRejectionCount == 0) {
    editor.remove(mDevice.getAddress());
  }
 else {
    editor.putInt(mDevice.getAddress(),mMessageRejectionCount);
  }
  editor.commit();
}
