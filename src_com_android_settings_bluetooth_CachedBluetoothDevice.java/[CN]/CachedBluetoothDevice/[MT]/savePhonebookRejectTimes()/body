{
  SharedPreferences.Editor editor=mContext.getSharedPreferences(PHONEBOOK_REJECT_TIMES,Context.MODE_PRIVATE).edit();
  if (mPhonebookRejectedTimes == 0) {
    editor.remove(mDevice.getAddress());
  }
 else {
    editor.putInt(mDevice.getAddress(),mPhonebookRejectedTimes);
  }
  editor.commit();
}
