{
  if (preference == mDeviceNamePref) {
    mCachedDevice.setName((String)newValue);
  }
 else   if (preference instanceof CheckBoxPreference) {
    LocalBluetoothProfile prof=getProfileOf(preference);
    onProfileClicked(prof);
    return false;
  }
 else {
    return false;
  }
  return true;
}
