{
  mRunnable=null;
  LocalBluetoothProfileManager.removeServiceListener(this);
  if (mDialog != null) {
    mDialog.dismiss();
    mDialog=null;
  }
  mDevice=null;
  mPendingDevice=null;
  CachedBluetoothDevice cachedBluetoothDevice=getCachedBluetoothDevice(context,localManager,device);
  cachedBluetoothDevice.disconnect();
}
