{
  StringBuilder sb=new StringBuilder();
  sb.append(command.name());
  sb.append(" Address:").append(device.mAddress);
  sb.append(" Profile:").append(profile.name());
  sb.append(" TimeSent:");
  if (timeSent == 0) {
    sb.append("not yet");
  }
 else {
    sb.append(DateFormat.getTimeInstance().format(new Date(timeSent)));
  }
  return sb.toString();
}
