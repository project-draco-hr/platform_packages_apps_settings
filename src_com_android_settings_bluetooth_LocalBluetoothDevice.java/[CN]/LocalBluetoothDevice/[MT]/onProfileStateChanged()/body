{
  if (D) {
    Log.d(TAG,"onProfileStateChanged:" + workQueue.toString());
  }
  BluetoothJob job=workQueue.peek();
  if (job == null) {
    return;
  }
 else   if (job.device.mAddress != mAddress) {
    if (D) {
      Log.d(TAG,"mAddresses:" + mAddress + " != head:"+ job.toString());
    }
    if (!pruneQueue(null)) {
      return;
    }
  }
 else {
    workQueue.poll();
  }
  processCommands();
}
