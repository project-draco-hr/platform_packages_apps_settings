{
  SharedPreferences.Editor editor=getSharedPreferences().edit();
  editor.putString(LocalBluetoothManager.SHARED_PREFERENCES_KEY_LAST_SELECTED_DEVICE,deviceAddress);
  editor.putLong(LocalBluetoothManager.SHARED_PREFERENCES_KEY_LAST_SELECTED_DEVICE_TIME,System.currentTimeMillis());
  editor.commit();
}
