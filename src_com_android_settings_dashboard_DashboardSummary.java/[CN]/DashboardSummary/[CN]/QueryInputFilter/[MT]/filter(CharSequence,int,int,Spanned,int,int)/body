{
  SpannableStringBuilder sb=null;
  for (int i=start; i < end; i++) {
    char c=source.charAt(i);
    if (!Character.isLetterOrDigit(c) && !Character.isSpaceChar(c)) {
      if (sb == null) {
        sb=new SpannableStringBuilder(source,start,end);
      }
      sb.delete(i,i + 1);
    }
  }
  return sb;
}
