{
  if (query == null) {
    return null;
  }
  final StringBuilder filtered=new StringBuilder();
  for (int n=0; n < query.length(); n++) {
    char c=query.charAt(n);
    if (!Character.isLetterOrDigit(c) && !Character.isSpaceChar(c)) {
      continue;
    }
    filtered.append(c);
  }
  return filtered.toString();
}
