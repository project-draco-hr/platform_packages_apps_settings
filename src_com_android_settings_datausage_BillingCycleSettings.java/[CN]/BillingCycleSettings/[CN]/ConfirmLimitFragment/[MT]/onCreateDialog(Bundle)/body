{
  final Context context=getActivity();
  final CharSequence message=getArguments().getCharSequence(EXTRA_MESSAGE);
  return new AlertDialog.Builder(context).setTitle(R.string.data_usage_limit_dialog_title).setMessage(message).setPositiveButton(android.R.string.ok,this).setNegativeButton(android.R.string.cancel,null).create();
}
