{
  if (mEntry.icon == null) {
    holder.itemView.post(new Runnable(){
      @Override public void run(){
        mApplicationsState.ensureIcon(mEntry);
        setIcon(mEntry.icon);
      }
    }
);
  }
  holder.findViewById(android.R.id.widget_frame).setVisibility(mState.isDataSaverBlacklisted ? View.INVISIBLE : View.VISIBLE);
  super.onBindViewHolder(holder);
}
