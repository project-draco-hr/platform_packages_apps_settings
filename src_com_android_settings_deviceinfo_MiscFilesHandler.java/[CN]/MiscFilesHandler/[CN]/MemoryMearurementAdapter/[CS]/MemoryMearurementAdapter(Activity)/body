{
  mContext=activity;
  final Bundle extras=activity.getIntent().getExtras();
  final StorageVolume storageVolume=extras.getParcelable(StorageVolumePreferenceCategory.STORAGE_VOLUME);
  StorageMeasurement mMeasurement=StorageMeasurement.getInstance(activity,storageVolume,false);
  mData=(ArrayList<StorageMeasurement.FileInfo>)mMeasurement.mFileInfoForMisc;
  if (mData != null) {
    for (    StorageMeasurement.FileInfo info : mData) {
      mDataSize+=info.mSize;
    }
  }
}
