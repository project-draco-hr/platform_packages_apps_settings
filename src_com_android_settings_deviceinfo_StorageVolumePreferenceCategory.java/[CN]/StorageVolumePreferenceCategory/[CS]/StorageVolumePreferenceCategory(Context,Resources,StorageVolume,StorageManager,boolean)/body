{
  super(context);
  mResources=resources;
  mStorageVolume=storageVolume;
  mStorageManager=storageManager;
  setTitle(storageVolume != null ? storageVolume.getDescription(context) : resources.getText(R.string.internal_storage));
  mLocalMeasure=StorageMeasurement.getInstance(context,storageVolume,new UserHandle(UserHandle.USER_CURRENT),isPrimary);
  mAllMeasures.add(mLocalMeasure);
  mAllowFormat=mStorageVolume != null && !mStorageVolume.isEmulated();
  if (!isPrimary)   mAllowFormat=false;
  mMeasureUsers=(mStorageVolume != null && mStorageVolume.isEmulated()) && isPrimary;
}
