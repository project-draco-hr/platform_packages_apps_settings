{
  long challenge=getSystemService(FingerprintManager.class).preEnroll();
  ChooseLockSettingsHelper helper=new ChooseLockSettingsHelper(this);
  if (!helper.launchConfirmationActivity(CONFIRM_REQUEST,getString(R.string.security_settings_fingerprint_preference_title),null,null,challenge)) {
    finish();
  }
 else {
    mLaunchedConfirmLock=true;
  }
}
