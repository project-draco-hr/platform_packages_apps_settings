{
  super.onCreate(savedInstanceState);
  setContentView(R.layout.fingerprint_enroll_find_sensor);
  setHeaderText(R.string.security_settings_fingerprint_enroll_find_sensor_title);
  mLaunchedConfirmLock=savedInstanceState != null && savedInstanceState.getBoolean(EXTRA_KEY_LAUNCHED_CONFIRM);
  if (mToken == null && !mLaunchedConfirmLock) {
    launchConfirmLock();
  }
  mAnimation=(FingerprintLocationAnimationView)findViewById(R.id.fingerprint_sensor_location_animation);
}
