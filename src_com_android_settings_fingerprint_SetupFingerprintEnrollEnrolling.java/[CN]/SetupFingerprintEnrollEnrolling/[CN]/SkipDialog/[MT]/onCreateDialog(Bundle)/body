{
  final AlertDialog dialog=new AlertDialog.Builder(getActivity()).setTitle(R.string.setup_fingerprint_enroll_enrolling_skip_title).setMessage(R.string.setup_fingerprint_enroll_enrolling_skip_message).setCancelable(false).setPositiveButton(R.string.skip_label,new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int id){
      Activity activity=getActivity();
      if (activity != null) {
        activity.setResult(RESULT_SKIP);
        activity.finish();
      }
    }
  }
).setNegativeButton(R.string.setup_fingerprint_enroll_enrolling_stay_button,new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int id){
    }
  }
).create();
  SystemBarHelper.hideSystemBars(dialog);
  return dialog;
}
