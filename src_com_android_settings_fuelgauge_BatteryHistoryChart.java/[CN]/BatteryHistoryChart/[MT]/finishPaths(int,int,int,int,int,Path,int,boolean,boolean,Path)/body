{
  if (curLevelPath != null) {
    if (lastBatX >= 0) {
      if (lastPath != null) {
        lastPath.lineTo(w,y);
      }
      curLevelPath.lineTo(w,y);
    }
    curLevelPath.lineTo(w,levelh);
    curLevelPath.lineTo(startX,levelh);
    curLevelPath.close();
  }
  if (lastCharging) {
    mChargingPath.lineTo(w,h - mChargingOffset);
  }
  if (lastScreenOn) {
    mScreenOnPath.lineTo(w,h - mScreenOnOffset);
  }
}
