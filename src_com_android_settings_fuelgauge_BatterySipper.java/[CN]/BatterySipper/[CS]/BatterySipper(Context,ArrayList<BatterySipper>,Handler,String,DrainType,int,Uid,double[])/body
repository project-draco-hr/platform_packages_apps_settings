{
  mContext=context;
  mRequestQueue=requestQueue;
  mHandler=handler;
  this.values=values;
  name=label;
  this.drainType=drainType;
  if (iconId > 0) {
    icon=mContext.getResources().getDrawable(iconId);
  }
  if (values != null)   value=values[0];
  if ((label == null || iconId == 0) && uid != null) {
    getQuickNameIconForUid(uid);
  }
  uidObj=uid;
}
