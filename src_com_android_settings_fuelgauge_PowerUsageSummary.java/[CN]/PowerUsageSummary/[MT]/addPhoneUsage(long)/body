{
  long phoneOnTimeMs=mStats.getPhoneOnTime(uSecNow,mStatsType) / 1000;
  double phoneOnPower=mPowerProfile.getAveragePower(PowerProfile.POWER_RADIO_ACTIVE) * phoneOnTimeMs / 1000;
  addEntry(getString(R.string.power_phone),DrainType.PHONE,phoneOnTimeMs,android.R.drawable.ic_menu_call,phoneOnPower);
}
