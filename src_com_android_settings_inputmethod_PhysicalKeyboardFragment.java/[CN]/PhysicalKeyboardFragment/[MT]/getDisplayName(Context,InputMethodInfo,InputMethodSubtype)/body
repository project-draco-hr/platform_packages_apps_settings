{
  final CharSequence imeName=imi.loadLabel(context.getPackageManager());
  if (imSubtype == null) {
    return imeName;
  }
  final CharSequence imSubtypeName=imSubtype.getDisplayName(context,imi.getPackageName(),imi.getServiceInfo().applicationInfo);
  return String.format(context.getString(R.string.physical_device_title),imSubtypeName,imeName);
}
