{
  if (mFragment != null && mSettingsIntent != null) {
    try {
      mFragment.startActivity(mSettingsIntent);
    }
 catch (    ActivityNotFoundException e) {
      final String msg=mFragment.getString(R.string.failed_to_open_app_settings_toast,mSpellCheckerInfo.loadLabel(mFragment.getActivity().getPackageManager()));
      Toast.makeText(mFragment.getActivity(),msg,Toast.LENGTH_LONG).show();
    }
  }
}
