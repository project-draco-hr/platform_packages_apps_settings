{
  if (mManagedProfile == null) {
    return null;
  }
  DevicePolicyManager dpm=(DevicePolicyManager)getActivity().getSystemService(Context.DEVICE_POLICY_SERVICE);
  if (dpm == null) {
    return null;
  }
  List<ComponentName> admins=dpm.getActiveAdminsAsUser(mManagedProfile.getIdentifier());
  for (int i=0; i < admins.size(); ++i) {
    final ComponentName admin=admins.get(i);
    Bundle restrictions=dpm.getUserRestrictions(admin,mManagedProfile.getIdentifier());
    if (restrictions != null && restrictions.getBoolean(UserManager.DISALLOW_SHARE_LOCATION,false)) {
      return admin;
    }
  }
  return null;
}
