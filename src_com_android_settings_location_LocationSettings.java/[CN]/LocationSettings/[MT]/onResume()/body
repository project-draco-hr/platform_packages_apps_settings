{
  super.onResume();
  createPreferenceHierarchy();
  updateLocationToggles();
  if (mSettingsObserver == null) {
    mSettingsObserver=new Observer(){
      @Override public void update(      Observable o,      Object arg){
        updateLocationToggles();
      }
    }
;
  }
  mContentQueryMap.addObserver(mSettingsObserver);
}
