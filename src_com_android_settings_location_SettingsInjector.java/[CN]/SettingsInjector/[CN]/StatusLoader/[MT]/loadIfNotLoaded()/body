{
  if (mLoaded) {
    return;
  }
  mContext.startService(mIntent);
  if (mPrev != null) {
    Handler handler=new Handler(){
      @Override public void handleMessage(      Message msg){
        mPrev.loadIfNotLoaded();
      }
    }
;
    handler.sendMessageDelayed(Message.obtain(handler),INJECTED_STATUS_UPDATE_TIMEOUT_MILLIS);
  }
  mLoaded=true;
}
