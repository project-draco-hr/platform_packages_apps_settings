{
  super.onCreate(savedInstanceState);
  final ActionBar actionBar=getActivity().getActionBar();
  mOldActivityTitle=actionBar.getTitle();
  actionBar.setTitle(R.string.android_beam_settings_title);
  mNfcAdapter=NfcAdapter.getDefaultAdapter(getActivity());
  mBeamDisallowed=((UserManager)getActivity().getSystemService(Context.USER_SERVICE)).hasUserRestriction(UserManager.DISALLOW_OUTGOING_BEAM);
}
