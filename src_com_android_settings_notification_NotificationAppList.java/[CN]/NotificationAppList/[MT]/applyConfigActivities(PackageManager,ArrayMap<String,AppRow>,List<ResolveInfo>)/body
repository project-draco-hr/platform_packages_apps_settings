{
  if (DEBUG)   Log.d(TAG,"Found " + resolveInfos.size() + " preference activities"+ (resolveInfos.size() == 0 ? " ;_;" : ""));
  for (  ResolveInfo ri : resolveInfos) {
    final ActivityInfo activityInfo=ri.activityInfo;
    final ApplicationInfo appInfo=activityInfo.applicationInfo;
    final AppRow row=rows.get(appInfo.packageName);
    if (row == null) {
      Log.v(TAG,"Ignoring notification preference activity (" + activityInfo.name + ") for unknown package "+ activityInfo.packageName);
      continue;
    }
    if (row.settingsIntent != null) {
      Log.v(TAG,"Ignoring duplicate notification preference activity (" + activityInfo.name + ") for package "+ activityInfo.packageName);
      continue;
    }
    row.settingsIntent=new Intent(APP_NOTIFICATION_PREFS_CATEGORY_INTENT).setClassName(activityInfo.packageName,activityInfo.name);
  }
}
