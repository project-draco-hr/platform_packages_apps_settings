{
  mRingOrNotificationPreference.showIcon(mSuppressor != null ? com.android.internal.R.drawable.ic_audio_ring_notif_mute : mRingOrNotificationProgress > 0 ? com.android.internal.R.drawable.ic_audio_ring_notif : (mVibrator == null ? com.android.internal.R.drawable.ic_audio_ring_notif_mute : com.android.internal.R.drawable.ic_audio_ring_notif_vibrate));
}
