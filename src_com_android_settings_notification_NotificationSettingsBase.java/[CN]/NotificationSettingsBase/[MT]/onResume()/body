{
  super.onResume();
  if ((mUid != -1 && getPackageManager().getPackagesForUid(mUid) == null)) {
    finish();
    return;
  }
  mSuspendedAppsAdmin=RestrictedLockUtils.checkIfApplicationIsSuspended(mContext,mPkg,mUserId);
  if (mImportance != null) {
    mImportance.setDisabledByAdmin(mSuspendedAppsAdmin);
  }
  if (mPriority != null) {
    mPriority.setDisabledByAdmin(mSuspendedAppsAdmin);
  }
  if (mSensitive != null) {
    mSensitive.setDisabledByAdmin(mSuspendedAppsAdmin);
  }
  if (mImportanceTitle != null) {
    mImportanceTitle.setDisabledByAdmin(mSuspendedAppsAdmin);
  }
}
