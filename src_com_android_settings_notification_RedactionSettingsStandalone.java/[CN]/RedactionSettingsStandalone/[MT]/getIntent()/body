{
  Intent modIntent=new Intent(super.getIntent());
  modIntent.putExtra(EXTRA_SHOW_FRAGMENT,RedactionInterstitialFragment.class.getName()).putExtra(EXTRA_PREFS_SHOW_BUTTON_BAR,true).putExtra(EXTRA_PREFS_SET_BACK_TEXT,(String)null).putExtra(EXTRA_PREFS_SET_NEXT_TEXT,getString(R.string.app_notifications_dialog_done));
  return modIntent;
}
