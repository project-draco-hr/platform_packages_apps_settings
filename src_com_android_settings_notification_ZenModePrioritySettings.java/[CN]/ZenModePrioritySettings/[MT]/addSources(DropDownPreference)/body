{
  pref.setEntryValues(new CharSequence[]{pref.getContext().getString(R.string.zen_mode_from_anyone),pref.getContext().getString(R.string.zen_mode_from_contacts),pref.getContext().getString(R.string.zen_mode_from_starred),pref.getContext().getString(R.string.zen_mode_from_none)});
  pref.setEntryValues(new CharSequence[]{Integer.toString(ZenModeConfig.SOURCE_ANYONE),Integer.toString(ZenModeConfig.SOURCE_CONTACT),Integer.toString(ZenModeConfig.SOURCE_STAR),Integer.toString(SOURCE_NONE)});
}
