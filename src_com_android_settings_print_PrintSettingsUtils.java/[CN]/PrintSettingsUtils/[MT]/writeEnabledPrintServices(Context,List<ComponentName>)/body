{
  StringBuilder builder=new StringBuilder();
  final int serviceCount=services.size();
  for (int i=0; i < serviceCount; i++) {
    ComponentName service=services.get(i);
    if (builder.length() > 0) {
      builder.append(ENABLED_PRINT_SERVICES_SEPARATOR);
    }
    builder.append(service.flattenToString());
  }
  Settings.Secure.putString(context.getContentResolver(),Settings.Secure.ENABLED_PRINT_SERVICES,builder.toString());
}
