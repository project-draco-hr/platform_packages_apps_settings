{
  super(context);
  mContext=context;
  mSubInfoRecord=subInfoRecord;
  mSlotId=slotId;
  setKey("sim" + mSlotId);
  update();
  mTintArr=context.getResources().getIntArray(com.android.internal.R.array.sim_colors);
  mColorStrings=context.getResources().getStringArray(R.array.color_picker);
  mTintSelectorPos=0;
}
