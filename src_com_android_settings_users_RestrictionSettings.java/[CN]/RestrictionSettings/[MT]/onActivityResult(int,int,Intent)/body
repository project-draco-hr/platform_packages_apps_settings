{
  if (requestCode == REQUEST_PIN_CHALLENGE) {
    mChallengeRequested=false;
    if (resultCode == Activity.RESULT_OK) {
      getListView().setEnabled(true);
      mChallengeSucceeded=true;
    }
 else {
      finishFragment();
    }
    return;
  }
  super.onActivityResult(requestCode,resultCode,data);
}
