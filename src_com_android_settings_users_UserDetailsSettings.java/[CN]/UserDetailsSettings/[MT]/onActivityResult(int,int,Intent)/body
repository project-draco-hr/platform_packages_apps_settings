{
  if (resultCode != Activity.RESULT_OK) {
    return;
  }
switch (requestCode) {
case RESULT_PICK_IMAGE:
    if (data.getData() != null) {
      Uri imageUri=data.getData();
      System.err.println("imageUri = " + imageUri);
      cropImage(imageUri);
    }
  break;
case RESULT_CROP_IMAGE:
saveCroppedImage(data);
break;
}
}
