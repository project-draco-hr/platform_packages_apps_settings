{
  Cursor cursor=getActivity().getContentResolver().query(Profile.CONTENT_URI,CONTACT_PROJECTION,null,null,null);
  if (cursor == null) {
    Log.w(TAG,"getProfileName() returned NULL cursor!" + " contact uri used " + Profile.CONTENT_URI);
    return null;
  }
  try {
    if (cursor.moveToFirst()) {
      return cursor.getString(cursor.getColumnIndex(Phone.DISPLAY_NAME));
    }
  }
  finally {
    cursor.close();
  }
  return null;
}
