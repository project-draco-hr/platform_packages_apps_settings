{
  super(context);
  setLayoutResource(R.layout.preference_tts_engine);
  mSharedState=state;
  mLabel=info.label;
  mAppLabel=info.appLabel;
  mAlertText=alertText;
  mSettingsComponent=info.settings;
  mPreventRadioButtonCallbacks=false;
  setKey(info.key);
  setTitle(info.label);
  setSummary(summary);
}
