{
  final String vpnLabel=getArguments().getString(ARG_VPN_LABEL);
  return new AlertDialog.Builder(getActivity()).setTitle(getActivity().getString(R.string.vpn_cant_connect_title,vpnLabel)).setMessage(getActivity().getString(R.string.vpn_cant_connect_message)).setPositiveButton(R.string.okay,null).create();
}
