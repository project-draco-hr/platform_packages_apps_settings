{
  return new AlertDialog.Builder(getActivity()).setTitle(R.string.vpn_replace_always_on_vpn_title).setMessage(getActivity().getString(R.string.vpn_replace_always_on_vpn_message)).setNegativeButton(getActivity().getString(R.string.vpn_cancel),null).setPositiveButton(getActivity().getString(R.string.vpn_continue),this).create();
}
