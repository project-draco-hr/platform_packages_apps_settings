{
  mCaCertificate=createListPreference(c,R.string.vpn_ca_certificate_title,mProfile.getCaCertificate(),CertTool.getInstance().getAllCaCertificateKeys(),new Preference.OnPreferenceChangeListener(){
    public boolean onPreferenceChange(    Preference pref,    Object newValue){
      mProfile.setCaCertificate((String)newValue);
      setSummary(pref,R.string.vpn_ca_certificate,(String)newValue);
      return true;
    }
  }
);
  setSummary(mCaCertificate,R.string.vpn_ca_certificate,mProfile.getCaCertificate());
  return mCaCertificate;
}
