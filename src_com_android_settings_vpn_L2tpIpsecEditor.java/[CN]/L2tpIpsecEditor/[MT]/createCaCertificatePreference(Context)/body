{
  mCaCertificate=createListPreference(c,R.string.vpn_ca_certificate_title,mProfile.getCaCertificate(),Keystore.getInstance().getAllCaCertificateKeys(),new Preference.OnPreferenceChangeListener(){
    public boolean onPreferenceChange(    Preference pref,    Object newValue){
      mProfile.setCaCertificate((String)newValue);
      return onPreferenceChangeCommon(pref,newValue);
    }
  }
);
  return mCaCertificate;
}
