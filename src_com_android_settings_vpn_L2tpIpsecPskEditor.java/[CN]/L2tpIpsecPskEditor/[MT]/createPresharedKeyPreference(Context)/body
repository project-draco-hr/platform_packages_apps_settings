{
  SecretHandler pskHandler=mPskHandler=new SecretHandler(c,R.string.vpn_ipsec_presharedkey_title,R.string.vpn_ipsec_presharedkey){
    @Override protected String getSecretFromProfile(){
      return ((L2tpIpsecPskProfile)getProfile()).getPresharedKey();
    }
    @Override protected void saveSecretToProfile(    String secret){
      ((L2tpIpsecPskProfile)getProfile()).setPresharedKey(secret);
    }
  }
;
  return pskHandler.getPreference();
}
