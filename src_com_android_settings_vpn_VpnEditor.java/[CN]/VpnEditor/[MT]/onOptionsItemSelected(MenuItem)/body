{
switch (item.getItemId()) {
case MENU_SAVE:
    Intent resultIntent=validateAndGetResult();
  if (resultIntent != null) {
    ((PreferenceActivity)getActivity()).finishPreferencePanel(this,Activity.RESULT_OK,resultIntent);
  }
return true;
case MENU_CANCEL:
if (profileChanged()) {
showDialog(CONFIRM_DIALOG_ID);
}
 else {
finishFragment();
}
return true;
}
return super.onOptionsItemSelected(item);
}
