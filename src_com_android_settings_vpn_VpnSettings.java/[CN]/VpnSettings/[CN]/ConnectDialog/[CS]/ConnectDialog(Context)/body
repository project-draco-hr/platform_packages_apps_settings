{
  super(context);
  setTitle(String.format(getString(R.string.vpn_connect_to),mConnectingActor.getProfile().getName()));
  setButton(DialogInterface.BUTTON_POSITIVE,getString(R.string.vpn_connect_button),VpnSettings.this);
  setButton(DialogInterface.BUTTON_NEGATIVE,getString(android.R.string.cancel),VpnSettings.this);
  setView(mConnectingActor.createConnectView());
}
