{
  final Activity activity=getActivity();
  return new AlertDialog.Builder(activity).setView(mConnectingActor.createConnectView()).setTitle(String.format(activity.getString(R.string.vpn_connect_to),mConnectingActor.getProfile().getName())).setPositiveButton(activity.getString(R.string.vpn_connect_button),this).setNegativeButton(activity.getString(android.R.string.cancel),this).create();
}
