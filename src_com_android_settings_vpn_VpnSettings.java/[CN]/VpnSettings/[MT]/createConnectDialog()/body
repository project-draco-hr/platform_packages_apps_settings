{
  return new AlertDialog.Builder(this).setView(mConnectingActor.createConnectView()).setTitle(String.format(getString(R.string.vpn_connect_to),mConnectingActor.getProfile().getName())).setPositiveButton(getString(R.string.vpn_connect_button),this).setNegativeButton(getString(android.R.string.cancel),this).create();
}
