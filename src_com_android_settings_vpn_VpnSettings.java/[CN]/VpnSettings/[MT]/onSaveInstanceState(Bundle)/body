{
  if (mActiveProfile != null) {
    savedInstanceState.putString(KEY_ACTIVE_PROFILE,mActiveProfile.getId());
    savedInstanceState.putBoolean(KEY_PROFILE_CONNECTING,(mConnectingActor != null));
    savedInstanceState.putBoolean(KEY_CONNECT_DIALOG_SHOWING,mConnectDialogShowing);
  }
  super.onSaveInstanceState(savedInstanceState);
}
