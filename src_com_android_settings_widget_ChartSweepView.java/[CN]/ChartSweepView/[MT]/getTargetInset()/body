{
  if (mFollowAxis == VERTICAL) {
    final float targetHeight=mSweep.getIntrinsicHeight() - mSweepMargins.top - mSweepMargins.bottom;
    return mSweepMargins.top + (targetHeight / 2);
  }
 else {
    final float targetWidth=mSweep.getIntrinsicWidth() - mSweepMargins.left - mSweepMargins.right;
    return mSweepMargins.left + (targetWidth / 2);
  }
}
