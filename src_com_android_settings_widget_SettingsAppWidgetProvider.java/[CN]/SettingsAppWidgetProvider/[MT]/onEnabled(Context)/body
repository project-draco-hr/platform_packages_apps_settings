{
  Class clazz=com.android.settings.widget.SettingsAppWidgetProvider.class;
  PackageManager pm=context.getPackageManager();
  pm.setComponentEnabledSetting(new ComponentName(context.getPackageName(),clazz.getName()),PackageManager.COMPONENT_ENABLED_STATE_ENABLED,PackageManager.DONT_KILL_APP);
  checkObserver(context);
}
