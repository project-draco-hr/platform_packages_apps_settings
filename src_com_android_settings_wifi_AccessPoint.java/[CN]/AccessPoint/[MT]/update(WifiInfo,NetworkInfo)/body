{
  boolean reorder=false;
  if (info != null && isInfoForThisAccessPoint(info)) {
    reorder=(mInfo == null);
    mRssi=info.getRssi();
    mInfo=info;
    mNetworkInfo=networkInfo;
    refresh();
  }
 else   if (mInfo != null) {
    reorder=true;
    mInfo=null;
    mNetworkInfo=null;
    refresh();
  }
  if (reorder) {
    notifyHierarchyChanged();
  }
}
