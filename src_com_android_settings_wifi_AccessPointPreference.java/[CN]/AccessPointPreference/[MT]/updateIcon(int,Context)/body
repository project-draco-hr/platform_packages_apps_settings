{
  if (level == -1) {
    setIcon(null);
  }
 else {
    if (getIcon() == null) {
      StateListDrawable sld=(StateListDrawable)context.getTheme().obtainStyledAttributes(wifi_signal_attributes).getDrawable(0);
      if (sld != null) {
        sld.setState((mAccessPoint.getSecurity() != AccessPoint.SECURITY_NONE) ? STATE_SECURED : STATE_NONE);
        Drawable drawable=sld.getCurrent();
        if (!mForSavedNetworks) {
          setIcon(drawable);
        }
 else {
          setIcon(null);
        }
      }
    }
  }
}
