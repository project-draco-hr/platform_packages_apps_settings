{
  if (level == -1) {
    setIcon(null);
  }
 else {
    if (getIcon() == null) {
      if (mWifiSld != null) {
        mWifiSld.setState((mAccessPoint.getSecurity() != AccessPoint.SECURITY_NONE) ? STATE_SECURED : STATE_NONE);
        Drawable drawable=mWifiSld.getCurrent();
        if (!mForSavedNetworks) {
          setIcon(drawable);
        }
 else {
          setIcon(null);
        }
      }
    }
  }
}
