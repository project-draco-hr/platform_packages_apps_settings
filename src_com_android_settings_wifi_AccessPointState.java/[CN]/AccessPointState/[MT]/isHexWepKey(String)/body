{
  final int len=wepKey.length();
  if (len != 10 && len != 26 && len != 58) {
    return false;
  }
  for (int i=len - 1; i >= 0; i--) {
    final char c=wepKey.charAt(i);
    if (!(c >= '0' && c <= '9' || c >= 'A' && c <= 'F' || c >= 'a' && c <= 'f')) {
      return false;
    }
  }
  return true;
}
