{
switch (dialogId) {
case WifiSettings.WIFI_DIALOG_ID:
    if (mDlgAccessPoint == null) {
      mDlgAccessPoint=new AccessPoint(getActivity(),mAccessPointSavedState);
      mAccessPointSavedState=null;
    }
  mSelectedAccessPoint=mDlgAccessPoint;
final boolean hideForgetButton=WifiSettings.isEditabilityLockedDown(getActivity(),mDlgAccessPoint.getConfig());
mDialog=new WifiDialog(getActivity(),this,mDlgAccessPoint,false,false,true,hideForgetButton);
return mDialog;
}
return super.onCreateDialog(dialogId);
}
