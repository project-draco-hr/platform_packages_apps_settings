{
  if ((mSsid != null && mSsid.length() == 0) || (mSecurityType == AccessPoint.SECURITY_WEP && mPassword.length() == 0) || (mSecurityType == AccessPoint.SECURITY_PSK && mPassword.length() < 8)) {
    getButton(BUTTON_SUBMIT).setEnabled(false);
  }
 else {
    getButton(BUTTON_SUBMIT).setEnabled(true);
  }
}
