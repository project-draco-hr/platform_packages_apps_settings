{
  if ((mSsidView != null && mSsidView.length() == 0) || ((mAccessPoint == null || mAccessPoint.networkId == -1) && ((mAccessPointSecurity == AccessPoint.SECURITY_WEP && mPasswordView.length() == 0) || (mAccessPointSecurity == AccessPoint.SECURITY_PSK && mPasswordView.length() < 8)))) {
    mConfigUi.getSubmitButton().setEnabled(false);
  }
 else {
    mConfigUi.getSubmitButton().setEnabled(true);
  }
}
