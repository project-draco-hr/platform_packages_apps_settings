{
  mView.findViewById(R.id.no_ca_cert_warning).setVisibility(View.GONE);
  if (mEapCaCertSpinner != null && mView.findViewById(R.id.l_ca_cert).getVisibility() != View.GONE && ((String)mEapCaCertSpinner.getSelectedItem()).equals(mDoNotValidateEapServerString)) {
    mView.findViewById(R.id.no_ca_cert_warning).setVisibility(View.VISIBLE);
  }
}
