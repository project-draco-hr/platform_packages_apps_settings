{
  List<AccessPointState> accessPoints=new ArrayList<AccessPointState>();
synchronized (this) {
    accessPoints.addAll(mApScanList);
    accessPoints.addAll(mApOtherList);
    mApScanList.clear();
    mApOtherList.clear();
  }
  for (int i=accessPoints.size() - 1; i >= 0; i--) {
    removeApFromUi(accessPoints.get(i));
  }
}
