{
  super.onResume();
  if (mWifiEnabler != null) {
    mWifiEnabler.resume();
  }
  registerReceiver(mReceiver,mFilter);
  if (mKeyStoreNetworkId != -1 && KeyStore.getInstance().test() == KeyStore.NO_ERROR) {
    mWifiManager.connectNetwork(mKeyStoreNetworkId);
  }
  mKeyStoreNetworkId=-1;
  updateAccessPoints();
}
