{
  if (state.isEnterprise() && mKeyStore.test() != KeyStore.NO_ERROR) {
    Credentials.getInstance().unlock(this);
    mResumeState=state;
    mResumeMode=mode;
    return;
  }
  AccessPointDialog dialog=new AccessPointDialog(this,mWifiLayer);
  dialog.setMode(mode);
  dialog.setState(state);
  showDialog(dialog);
}
