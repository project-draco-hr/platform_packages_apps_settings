{
  Log.d(TAG,"finishOrNext resultCode=" + resultCode + " isUsingWizardManager="+ SetupWizardUtils.isUsingWizardManager(this));
  if (SetupWizardUtils.isUsingWizardManager(this)) {
    SetupWizardUtils.sendResultsToSetupWizard(this,resultCode);
  }
 else {
    setResult(resultCode);
    finish();
  }
}
