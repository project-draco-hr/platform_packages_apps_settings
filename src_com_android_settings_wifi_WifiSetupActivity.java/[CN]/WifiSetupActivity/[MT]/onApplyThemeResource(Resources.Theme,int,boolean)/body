{
  String themeName=getIntent().getStringExtra(EXTRA_THEME);
  if (THEME_HOLO_LIGHT.equalsIgnoreCase(themeName) || THEME_MATERIAL_LIGHT.equalsIgnoreCase(themeName)) {
    resid=R.style.SetupWizardWifiTheme_Light;
  }
 else   if (THEME_HOLO.equalsIgnoreCase(themeName) || THEME_MATERIAL.equalsIgnoreCase(themeName)) {
    resid=R.style.SetupWizardWifiTheme;
  }
  super.onApplyThemeResource(theme,resid,first);
}
