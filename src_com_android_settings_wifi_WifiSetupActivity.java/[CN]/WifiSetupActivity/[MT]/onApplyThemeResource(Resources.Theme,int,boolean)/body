{
  String themeName=getIntent().getStringExtra(EXTRA_THEME);
  if (THEME_HOLO_LIGHT.equalsIgnoreCase(themeName)) {
    resid=getResources().getIdentifier(RESOURCE_THEME_LIGHT,"style",getPackageName());
  }
 else   if (THEME_HOLO.equalsIgnoreCase(themeName)) {
    resid=getResources().getIdentifier(RESOURCE_THEME_DARK,"style",getPackageName());
  }
  super.onApplyThemeResource(theme,resid,first);
}
