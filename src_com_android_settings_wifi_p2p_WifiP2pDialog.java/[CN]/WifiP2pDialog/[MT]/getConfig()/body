{
  WifiP2pConfig config=new WifiP2pConfig();
  config.deviceAddress=mDeviceAddress.getText().toString();
  config.wps=new Wps();
switch (mWpsSetupIndex) {
case WPS_PBC:
    config.wps.setup=Setup.PBC;
  break;
case WPS_KEYPAD:
config.wps.setup=Setup.KEYPAD;
config.wps.pin=((TextView)mView.findViewById(R.id.wps_pin)).getText().toString();
break;
case WPS_DISPLAY:
config.wps.setup=Setup.DISPLAY;
break;
default :
config.wps.setup=Setup.PBC;
break;
}
return config;
}
