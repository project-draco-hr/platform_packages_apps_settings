{
  menu.add(Menu.NONE,MENU_ID_SEARCH,0,R.string.wifi_p2p_menu_search).setShowAsAction(MenuItem.SHOW_AS_ACTION_IF_ROOM);
  menu.add(Menu.NONE,MENU_ID_CREATE_GROUP,0,R.string.wifi_p2p_menu_create_group).setShowAsAction(MenuItem.SHOW_AS_ACTION_IF_ROOM);
  menu.add(Menu.NONE,MENU_ID_REMOVE_GROUP,0,R.string.wifi_p2p_menu_remove_group).setShowAsAction(MenuItem.SHOW_AS_ACTION_IF_ROOM);
  menu.add(Menu.NONE,MENU_ID_ADVANCED,0,R.string.wifi_p2p_menu_advanced).setShowAsAction(MenuItem.SHOW_AS_ACTION_IF_ROOM);
  super.onCreateOptionsMenu(menu,inflater);
}
